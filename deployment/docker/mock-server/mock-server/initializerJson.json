[
  {
    "id": "whatsapp-send-message",
    "httpRequest": {
      "method": "POST",
      "path": "/v1/messages"
    },
    "httpResponse": {
      "statusCode": 200,
      "headers": {
        "Content-Type": ["application/json"]
      },
      "body": {
        "success": true,
        "message_id": "wamid.mock123456789",
        "status": "sent"
      }
    },
    "times": {
      "unlimited": true
    }
  },
  {
    "id": "whatsapp-webhook-validation",
    "httpRequest": {
      "method": "GET",
      "path": "/webhook",
      "queryStringParameters": {
        "hub.mode": ["subscribe"],
        "hub.verify_token": [".*"]
      }
    },
    "httpResponse": {
      "statusCode": 200,
      "body": "${!request.queryStringParameters.hub.challenge[0]}"
    }
  },
